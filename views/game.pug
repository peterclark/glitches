main.game(v-if="authenticated")
  nav.navigation
    a.btn.btn-link(href='/genres') Genres
    button.btn.btn-link.logout-btn(href='/login')
      img.rounded(:src="user.photoURL")
      i.fa.fa-fw.fa-sign-out
      | {{ user.displayName }}

  .container
    .row
      .col-sm-6.col-md-4.col-lg-3(v-for="genre in genres")
        include genre.pug